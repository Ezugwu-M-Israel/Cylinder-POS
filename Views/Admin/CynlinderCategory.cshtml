@model IEnumerable<GasPOS.ViewModel.CynlinderCategoryViewModel>

@{
    ViewData["Title"] = "CynlinderCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<main id="main" class="main">

    <button type="button" class="btn rounded-pill text-white offset-5" data-bs-toggle="modal" data-bs-target="#smallModal" style="background-color: #012970;">
        Add Cynlinder Category
    </button>



@*    <div class="page-header">
        <div class="row align-items-center">
            <div class="col-auto float-right ml-auto">
                <a href="#" class="btn add-btnrounded-pill text-white" style="background-color: #012970;" data-bs-toggle="modal" data-bs-target="#create_GasCategory"><i class="fa fa-plus"></i>Add Cynlinder Category</a>
            </div>
        </div>
    </div>*@

    <div class="container-fluid">
      <div class="row pt-4 mt-5">

            <div class="col-12">
                <h2 class="text-white text-center py-2" style="margin-top: 70px; background-color:#012970;">CYNLINDER CATEGORY</h2>
            </div>

            @if (Model.Count() > 0)
            {
                <table class="table table-striped table-bordered" style="width:100%">

                    <thead>
                        <tr>
                            <th class="text-center">Name</th>
                            <th class="text-center">Quantity</th>
                            <th class="text-center">Active</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var gas in Model)
                        {
                            <tr>
                                <td width="5%" class="text-center">@gas.Name</td>
                                <td width="5%" class="text-center">@gas.Quantity</td>
                                <td width="5%" class="text-center">@gas.Active</td>
                            </tr>

                        }
                    </tbody>

                </table>
            }
      </div>

    </div>

    
</main>

 

